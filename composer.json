{
  "name": "elvandar/kazetenn_core_admin",
  "type": "symfony-bundle",
  "license": "MIT",
  "description": "A symfon administration builder library built for the kazetenn project",
  "authors": [
    {
      "name": "Elvandar Ysalys",
      "email": "elvandar.ysalys@protonmail.com"
    }
  ],
  "require": {
    "php": "8.1.*",
    "symfony/framework-bundle": "6.*",
    "symfony/translation": "6.*",
    "symfony/security-bundle": "6.*",
    "symfony/dependency-injection": "6.*",
    "symfony/config": "6.*",
    "symfony/http-kernel": "6.*",
    "symfony/http-foundation": "6.*",
    "symfony/routing": "6.*",
    "twig/extra-bundle": "3.*",
    "twig/twig": "3.*"
  },
  "autoload": {
    "psr-4": {
      "Kazetenn\\Admin\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "require-dev": {
    "symfony/webpack-encore-bundle": "^1.13",
    "phpro/grumphp": "^1.13",
    "phpstan/phpstan": "^1.8",
    "phpunit/phpunit": "^9.5",
    "rector/rector": "^0.12.23",
    "symfony/browser-kit": "6.*",
    "symfony/css-selector": "6.*",
    "symfony/debug-bundle": "6.*",
    "symfony/maker-bundle": "^1.0",
    "symfony/phpunit-bridge": "^6.0",
    "symfony/stopwatch": "6.*",
    "symfony/web-profiler-bundle": "6.*"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "symfony/runtime": true,
      "phpro/grumphp": true
    }
  }
}
